" placeholder=\"请输入密码\" class=\"w-75\"></el-input>\r\n  </el-form-item>\r\n  <el-button type=\"warning\" icon=\"\" @click=\"login\">登录</el-button>\r\n  <el-button type=\"primary\" @click=\"register\">注册</el-button>\r\n  <div class=\"m-5\"></div>\r\n  \r\n</el-form>\r\n    </el-col>\r\n    </div>\r\n    <div v-show=\"isLogin\">\r\n      <el-col class=\"text-center\" >\r\n        <h3>\r\n          亲爱的<span class=\"text-warning h2\">{{ formLabelAlign.username }}</span>!欢迎来到学生管理系统!\r\n        </h3>\r\n        \r\n        <video src=\"../../public/guidance.mp4\" width=\"700px\" controls autoplay loop muted></video>\r\n      </el-col>\r\n  </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport md5 from \"js-md5\"\r\nimport axios from 'axios'\r\n export default {\r\n   name:\"Login\",\r\n   data() {\r\n      return {\r\n        labelPosition: 'right',\r\n        formLabelAlign: {\r\n          username:\"\", \r\n          password:\"\",\r\n        },\r\n        isLogin:false\r\n      };\r\n    },\r\n    methods:{\r\n     async login(){\r\n        await axios({\r\n              url: \"http://localhost:9000/login\",\r\n              method: \"POST\",\r\n              data:{\r\n                username:this.formLabelAlign.username,\r\n                password:md5(this.formLabelAlign.password)\r\n              }\r\n            }).then(res=>{\r\n              if (res.data==\"error\") {\r\n                this.$alert(\"用户名或密码错误!\",\"温馨提示\",{\r\n                   confirmButtonText: '知道啦'\r\n                });\r\n              }\r\n              else{\r\n                sessionStorage.setItem(\"username\",res.data.username)\r\n                sessionStorage.setItem(\"token\",res.data.token);\r\n                this.$alert(\"登录成功！  尊敬的\"+sessionStorage.getItem(\"username\")+\"欢迎您！\",\"温馨提示\",{\r\n                   confirmButtonText: '知道啦'\r\n                });\r\n                this.isLogin=true;\r\n              }\r\n            })\r\n      },\r\n      register(){\r\n        this.$router.push({\r\n          name:\"register\",\r\n        })\r\n      },\r\n    },\r\n    mounted() {\r\n      this.$bus.$on(\"logout\",()=>{\r\n        this.isLogin=false\r\n      })\r\n    },\r\n  }\r\n</script>\r\n\r\n<style>\r\n\r\n</style>"]}]}